<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ProcessController">
    <center>
        <VBox alignment="CENTER" spacing="20">
            <HBox alignment="CENTER">
                <Button fx:id="classifyButton" text="Classify"/>
            </HBox>
            <HBox alignment="CENTER">
                <VBox>
                    <HBox spacing="5">
                        <Label fx:id="colLimitLabel"/>
                        <Slider fx:id="colLimitSlider"/>
                    </HBox>
                    <HBox spacing="5">
                        <Label fx:id="rowLimitLabel"/>
                        <Slider fx:id="rowLimitSlider"/>
                    </HBox>
                </VBox>
                <Button fx:id="pPDButton" text="Debug"/>
            </HBox>
        </VBox>
    </center>
</BorderPane>