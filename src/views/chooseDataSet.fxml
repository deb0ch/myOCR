<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<BorderPane fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ChooseDataSetDirectoryController">
    <center>
        <HBox  maxHeight="Infinity" maxWidth="Infinity" alignment="CENTER">
            <Label maxWidth="Infinity" maxHeight="Infinity" text="Choose the emplacement of your training set" textAlignment="CENTER"/>
        </HBox>
    </center>
    <bottom>
        <HBox>
            <Button fx:id="chooseDataSetDirectoryButton" text="Choose"/>
        </HBox>
    </bottom>
</BorderPane>